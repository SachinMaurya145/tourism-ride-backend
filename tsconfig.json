{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@config": [
        "config"
      ],
      "@config/*": [
        "config/*"
      ],
      "@constants": [
        "constants"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@middlewares": [
        "middlewares"
      ],
      "@middlewares/*": [
        "middlewares/*"
      ],
      "@modules": [
        "modules"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@utils": [
        "utils"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@types": [
        "types"
      ],
      "@types/*": [
        "types/*"
      ],
      "@jobs": [
        "jobs"
      ],
      "@jobs/*": [
        "jobs/*"
      ],
      "@seeds": [
        "seeds"
      ],
      "@seeds/*": [
        "seeds/*"
      ],
      "@sockets": [
        "sockets"
      ],
      "@sockets/*": [
        "sockets/*"
      ],
      "@swagger": [
        "swagger"
      ],
      "@swagger/*": [
        "swagger/*"
      ]
    }
  }
}